<div class="container mt-5">
  <h1> All events</h1> 
  <ul>
    <% for event in @events %>
    <li>
      <%= link_to event.artist, "/events/#{event.id}" %>
    </li>
    <% end %> 
  </ul>
  <button class="btn btn-default btn-lg" onclick="location.href='/events/new'">
    <%= link_to 'New event', new_event_path %>
  </button>
</div>
