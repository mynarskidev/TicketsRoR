<div class="container">
  <h1><%= header %></h1>
  <table class="table">
    <thead>
    <tr>
      <th>Artist</th>
      <th>Description</th>
      <th>Lowest Price</th>
      <th>Highest Price</th>
      <th>Event Date</th>
      <th>Event id</th>
    </tr>
    </thead>
    <tbody>
    <% events.each do |event| %>
      <tr>
      <td><%= event.artist %></td>
      <td><%= event.description %></td>
      <td><%= event.price_low %></td>
      <td><%= event.price_high %></td>
      <td><%= event.event_date %></td>
      <td><%= event.id %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
